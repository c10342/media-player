{{if !live && speedList && speedList.length>0}}
<div class="player-common-list-container">
  <ul class="player-common-list-wrapper player-speed-wrapper">
    {{each speedList}}
    <li class="player-common-list-item player-speed-item" data-index="{{ $index }}">
      {{ $value.label }}
    </li>
    {{/each}}
  </ul>
  <span class="player-common-list-label player-speed-label">
  </span>
</div>
{{/if}}
