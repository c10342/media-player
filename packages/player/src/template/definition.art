{{if videoList && videoList.length>0 && controls.definition}}
<div class="player-common-list-container">
  {{if videoList.length>1}}
  <ul class="player-common-list-wrapper player-definition-wrapper">
    {{each videoList}}
    <li class="player-common-list-item player-definition-item" data-index="{{ $index }}">
      {{ $value.label }}
    </li>
    {{/each}}
  </ul>
  {{/if}}
  <span class="player-common-list-label player-definition-label">
  </span>
</div>
{{/if}}
